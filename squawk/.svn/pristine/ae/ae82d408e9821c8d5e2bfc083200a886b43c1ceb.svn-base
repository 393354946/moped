#define  setLong(base_461, offset_463, value_465) { Address  base_462 = base_461;  Offset  offset_464 = offset_463;  jlong  value_466 = value_465;  \
            setLongTyped((base_462), (offset_464), AddressType_LONG, (value_466)); \
            setAssume((value_466) == getLong((base_462), (offset_464))); \
        }

        /**
         * Sets a 64 bit value in memory at a UWord offset.
         *
         * @param base   the base address
         * @param offset the offset (in UWords) from 'base' at which to write
         * @param value  the value to write
         */
#define  setLongAtWord(base_467, offset_469, value_471) { Address  base_468 = base_467;  Offset  offset_470 = offset_469;  jlong  value_472 = value_471;  \
            if (SQUAWK_64 || PLATFORM_UNALIGNED_64_LOADS) { \
                setLongAtWordTyped((base_468), (offset_470), AddressType_LONG, (value_472)); \
            } else { \
                const int highOffset = (PLATFORM_BIG_ENDIAN) ? (offset_470)     : (offset_470) + 1; \
                const int lowOffset  = (PLATFORM_BIG_ENDIAN) ? (offset_470) + 1 : (offset_470); \
                setIntTyped((base_468), highOffset, AddressType_LONG,  (int)((value_472) >> 32)); \
                setIntTyped((base_468), lowOffset,  AddressType_LONG2, (int) (value_472)); \
